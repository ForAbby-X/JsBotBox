{
	"compilerOptions": {
		"module": "es2015",
		"target": "es5",
		"sourceMap": true,

		"lib": ["es2015", "dom"],
		"baseUrl": "./",

		// Gives error if there are implied "<any>" types.
		"noImplicitAny": true, 

		// Allows you to access non-constant properties, such as object[keyVar]
		"ignoreDeprecations": "5.0",
		"suppressImplicitAnyIndexErrors": true
	},
	"exclude": [
		"node_modules"
	]
}